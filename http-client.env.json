{
  "dev": {
    "clientId": "netrunner-courses-client",
    "realm": "netrunner-realm",
    "tokenUrl": "https://auth.netrunner.site/realms/netrunner-realm/protocol/openid-connect/token",
    "authUrl" : "https://auth.netrunner.site/realms/netrunner-realm/protocol/openid-connect/auth",
    "logoutUrl": "https://auth.netrunner.site/realms/netrunner-realm/protocol/openid-connect/logout",
    "redirectUrl": "http://localhost/callback",
    "Security": {
      "Auth": {
        "access-token": {
          "Type": "OAuth2",
          "Grant Type": "Authorization Code",
          "Client ID": "{{clientId}}",
          "Redirect URL": "{{redirectUrl}}",
          "Token URL": "{{tokenUrl}}",
          "Auth URL": "{{authUrl}}",
          "Acquire Automatically": true
        },
        "refresh-token": {
          "Type": "OAuth2",
          "Grant Type": "Authorization Code",
          "Client ID": "{{clientId}}",
          "Token URL": "{{tokenUrl}}",
          "Auth URL": "{{authUrl}}",
          "Refresh URL": "{{tokenUrl}}",
          "Acquire Automatically": true
        }
      }
    }
  }
}