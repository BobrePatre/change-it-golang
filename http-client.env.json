{
  "dev": {
    "clientId": "changeit-client",
    "realm": "changeit-realm",
    "tokenUrl": "http://localhost:8180/realms/changeit-realm/protocol/openid-connect/token",
    "authUrl" : "http://localhost:8180/realms/changeit-realm/protocol/openid-connect/auth",
    "logoutUrl": "http://localhost:8180/realms/changeit-realm/protocol/openid-connect/logout",
    "redirectUrl": "http://localhost/callback",
    "Security": {
      "Auth": {
        "access-token": {
          "Type": "OAuth2",
          "Grant Type": "Authorization Code",
          "Client ID": "{{clientId}}",
          "Redirect URL": "{{redirectUrl}}",
          "Token URL": "{{tokenUrl}}",
          "Auth URL": "{{authUrl}}",
          "Acquire Automatically": true,
          "Scope": "email profile openid"
        },
        "refresh-token": {
          "Type": "OAuth2",
          "Grant Type": "Authorization Code",
          "Client ID": "{{clientId}}",
          "Token URL": "{{tokenUrl}}",
          "Auth URL": "{{authUrl}}",
          "Refresh URL": "{{tokenUrl}}",
          "Acquire Automatically": true
        }
      }
    }
  }
}